<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find">
        <Item>
          <IdeaActionID>ActivateFindToolWindow</IdeaActionID>
          <ShortCut>'⌘3'</ShortCut>
          <Description>Find</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'⌘;'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run">
        <Item>
          <IdeaActionID>ActivateRunToolWindow</IdeaActionID>
          <ShortCut>'⌘4'</ShortCut>
          <Description>Run</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Tomcat 9.0.6'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'Tomcat 9.0.6'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Tomcat 9.0.6'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Tomcat 9.0.6'</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="TODO">
        <Item>
          <IdeaActionID>ActivateTODOToolWindow</IdeaActionID>
          <ShortCut>'⌘6'</ShortCut>
          <Description>TODO</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>